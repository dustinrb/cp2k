&GLOBAL
  PRINT_LEVEL LOW
  PROJECT DDG_SOLVATION
  RUN_TYPE MD
&END GLOBAL

&EXT_RESTART
  RESTART_FILE_NAME DDG_SOLVATION-1.restart
&END EXT_RESTART

&MOTION
  &FREE_ENERGY
    METHOD AC
    &ALCHEMICAL_CHANGE
      PARAMETER k
      WEIGHTING_FUNCTION (1.0-k)*Z+k*J
    &END ALCHEMICAL_CHANGE
  &END FREE_ENERGY
  &MD
    ENSEMBLE NPT_I
    STEPS 5
    TEMPERATURE 200.0
    TIMESTEP 0.5
    &BAROSTAT
    &END BAROSTAT
    &PRINT
      &ENERGY
        &EACH
          MD 1
        &END EACH
      &END ENERGY
    &END PRINT
    &THERMOSTAT
      &NOSE
        LENGTH 3
        MTS 2
        TIMECON 300
        YOSHIDA 3
      &END NOSE
    &END THERMOSTAT
  &END MD
  &PRINT
    &FORCES OFF
    &END FORCES
    &RESTART
      &EACH
        MD 10
      &END EACH
    &END RESTART
    &RESTART_HISTORY OFF
    &END RESTART_HISTORY
    &TRAJECTORY
      &EACH
        MD 100
      &END EACH
    &END TRAJECTORY
    &VELOCITIES OFF
    &END VELOCITIES
  &END PRINT
&END MOTION

#CPQA DEPENDS Solv_alch_chng.inp

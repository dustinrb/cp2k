&GLOBAL
  PRINT_LEVEL LOW
  PROJECT deca_ala_intern
  RUN_TYPE md
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 10
    TEMPERATURE 298
    TIMESTEP 2.5
  &END MD
&END MOTION

&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      DO_NONBONDED F
      PARMTYPE CHM
      PARM_FILE_NAME ../sample_pot/deca_ala.pot
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE NONE
      &END EWALD
    &END POISSON
    &PRINT
      &FF_INFO
        SPLINE_DATA
        SPLINE_INFO
      &END FF_INFO
    &END PRINT
  &END MM
  &SUBSYS
    &CELL
      ABC 50.0 50.0 50.0
    &END CELL
    &TOPOLOGY
      CHARGE_BETA
      COORDINATE PDB
      COORD_FILE_NAME ../sample_pdb/deca_ala.pdb
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PRINT_LEVEL LOW
  PROJECT deca_ala4
  RUN_TYPE md
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NPT_F
    STEPS 10
    TEMPERATURE 298
    TIMESTEP 2.5
    &BAROSTAT
    &END BAROSTAT
    &THERMOSTAT
      TYPE CSVR
      &CSVR
      &END CSVR
    &END THERMOSTAT
  &END MD
  &PRINT
    &CELL
    &END CELL
  &END PRINT
&END MOTION

&FORCE_EVAL
  METHOD FIST
  STRESS_TENSOR ANALYTICAL
  &MM
    &FORCEFIELD
      PARMTYPE CHM
      PARM_FILE_NAME ../sample_pot/deca_ala.pot
    &END FORCEFIELD
    &POISSON
      &EWALD
        ALPHA .36
        EWALD_TYPE ewald
        GMAX 29
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 50.0 50.0 50.0
    &END CELL
    &TOPOLOGY
      CHARGE_BETA
      COORDINATE PDB
      COORD_FILE_NAME ../sample_pdb/deca_ala.pdb
      PARA_RES
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

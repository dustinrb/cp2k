&GLOBAL
  PRINT_LEVEL DEBUG
  PROJECT cubane_15
  RUN_TYPE GEO_OPT
  #RUN_TYPE MD
&END GLOBAL

&MOTION
  &GEO_OPT
    MAX_ITER 50
  &END GEO_OPT
  &MD
    ENSEMBLE NPT_F
    STEPS 5
    TIMESTEP 0.5
  &END MD
  &PRINT
    &TRAJECTORY
      FILENAME __STD_OUT__
      FORMAT ATOMIC
    &END TRAJECTORY
  &END PRINT
&END MOTION

&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      EI_SCALE14 0.83333333333333333333
      PARMTYPE CHM
      PARM_FILE_NAME ../sample_pot/cubane.pot
      SHIFT_CUTOFF .false.
      ! PARM91
      ! VDW_SCALE14 0.5
      ! EI_SCALE14  0.5
      ! PARM94 and Following
      VDW_SCALE14 0.5
    &END FORCEFIELD
    &POISSON
      &EWALD
        ALPHA .36
        EWALD_TYPE spme
        GMAX 64
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 30.0 30.0 30.0
    &END CELL
    &PRINT
      &TOPOLOGY_INFO
        UTIL_INFO
      &END TOPOLOGY_INFO
    &END PRINT
    &TOPOLOGY
      CONNECTIVITY PSF
      CONN_FILE_NAME ../sample_psf/cubane.psf
      COORDINATE PDB
      COORD_FILE_NAME ../sample_pdb/cubane_15.pdb
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

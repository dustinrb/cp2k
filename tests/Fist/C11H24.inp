&GLOBAL
  PRINT_LEVEL LOW
  PROJECT undecane
  RUN_TYPE GEO_OPT
&END GLOBAL

&MOTION
  &GEO_OPT
    MAX_ITER 0
    OPTIMIZER BFGS
  &END GEO_OPT
&END MOTION

&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      PARMTYPE CHM
      PARM_FILE_NAME sample_pot/butane.pot
    &END FORCEFIELD
    &POISSON
      &EWALD
        ALPHA .44
        EWALD_TYPE spme
        GMAX 24
        O_SPLINE 6
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 30.0 30.0 30.0
    &END CELL
    &TOPOLOGY
      CHARGE_BETA
      COORDINATE pdb
      COORD_FILE_NAME sample_pdb/C11H24.pdb
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PRINT_LEVEL LOW
  PROJECT ethene-vib-mode-sel-int
  RUN_TYPE NORMAL_MODES
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 100
    TEMPERATURE 298
    TIMESTEP 0.5
  &END MD
&END MOTION

&VIBRATIONAL_ANALYSIS
  DX 0.01
  INTENSITIES
  NPROC_REP 4
  &MODE_SELECTIVE
    FREQUENCY 3000
    !    ATOMS 2 3
    !     INITIAL_GUESS BFGS_HESS
  &END MODE_SELECTIVE
  &PRINT
    &PROGRAM_RUN_INFO ON
    &END PROGRAM_RUN_INFO
  &END PRINT
&END VIBRATIONAL_ANALYSIS

&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      PARMTYPE CHM
      PARM_FILE_NAME ../sample_pot/charmm.pot
      &CHARGE
        ATOM CE2
        CHARGE -0.42
      &END CHARGE
      &CHARGE
        ATOM HE2
        CHARGE 0.21
      &END CHARGE
    &END FORCEFIELD
    &POISSON
      &EWALD
        ALPHA .44
        EWALD_TYPE ewald
        GMAX 23
      &END EWALD
    &END POISSON
    &PRINT
      &DIPOLE
      &END DIPOLE
    &END PRINT
  &END MM
  &SUBSYS
    &CELL
      ABC 24.955 24.955 24.955
    &END CELL
    &TOPOLOGY
      COORDINATE pdb
      COORD_FILE_NAME ../sample_pdb/ethene.pdb
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

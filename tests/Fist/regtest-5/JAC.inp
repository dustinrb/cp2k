&GLOBAL
  PRINT_LEVEL LOW
  PROJECT JAC
  RUN_TYPE md
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 10
    TEMPERATURE 298
    TIMESTEP 0.5
  &END MD
  &PRINT
    &RESTART OFF
    &END RESTART
    &RESTART_HISTORY OFF
    &END RESTART_HISTORY
    &TRAJECTORY OFF
    &END TRAJECTORY
    &VELOCITIES OFF
    &END VELOCITIES
  &END PRINT
&END MOTION

&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      PARMTYPE CHM
      PARM_FILE_NAME ../sample_pot/par_all27_prot_lipid.pot
      &SPLINE
        RCUT_NB 9.0
      &END SPLINE
    &END FORCEFIELD
    &POISSON
      &EWALD
        ALPHA .4
        EWALD_TYPE spme
        GMAX 27
        O_SPLINE 4
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 62.23 62.23 62.23
    &END CELL
    &TOPOLOGY
      CONNECTIVITY UPSF
      CONN_FILE_NAME ../sample_psf/5dhfr_cube.psf
      COORDINATE PDB
      COORD_FILE_NAME ../sample_pdb/5dhfr_cube.pdb
      PARA_RES
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

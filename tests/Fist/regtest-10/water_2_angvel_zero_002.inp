&GLOBAL
  PRINT_LEVEL MEDIUM
  PROJECT water_2_angvel_zero_004
  RUN_TYPE md
&END GLOBAL

&EXT_RESTART
  RESTART_FILE_NAME water_2_angvel_zero_000-1.restart
&END EXT_RESTART

&MOTION
  &MD
    ANGVEL_ZERO T
    ENSEMBLE NVE
    STEPS 20
    TEMPERATURE 298
    TIMESTEP 2.5
  &END MD
&END MOTION

#CPQA DEPENDS water_2_angvel_zero_000.inp
&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      PARMTYPE CHM
      PARM_FILE_NAME ../sample_pot/water.pot
      &CHARGE
        ATOM OT
        CHARGE -0.8476
      &END CHARGE
      &CHARGE
        ATOM HT
        CHARGE 0.4238
      &END CHARGE
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE NONE
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 24.955 24.955 24.955
      PERIODIC NONE
    &END CELL
    &TOPOLOGY
      COORDINATE pdb
      COORD_FILE_NAME ../sample_pdb/water_2.pdb
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

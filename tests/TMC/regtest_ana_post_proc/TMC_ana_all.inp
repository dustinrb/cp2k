&GLOBAL
  PRINT_LEVEL LOW
  PROGRAM MC_ANALYSIS
  PROJECT H2O_TMC
  RUN_TYPE TMC
&END GLOBAL

&MOTION
  &TMC
    PRINT_TEST_OUTPUT
    &TMC_ANALYSIS_FILES
      CLASSICAL_DIPOLE_MOMENTS
      DENSITY 2 1 1
      DEVIATION
      FORCE_ENV_FILE H2O.inp
      G_R
      NR_TEMPERATURE 3
      TEMPERATURE 300 460
      &CHARGE
        ATOM O
        CHARGE -0.834
      &END CHARGE
      &CHARGE
        ATOM H
        CHARGE 0.417
      &END CHARGE
    &END TMC_ANALYSIS_FILES
  &END TMC
&END MOTION

&GLOBAL
  PRINT_LEVEL low
  PROJECT h2o_pbe0_admm-none_mom_s2
  RUN_TYPE energy
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    MULTIPLICITY 1
    POTENTIAL_FILE_NAME POTENTIAL
    UKS .true.
    WFN_RESTART_FILE_NAME h2o_pbe0_admm-none_gs-RESTART.wfn
    &AUXILIARY_DENSITY_MATRIX_METHOD
      ADMM_PURIFICATION_METHOD none
      METHOD basis_projection
    &END AUXILIARY_DENSITY_MATRIX_METHOD
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
      METHOD gpw
    &END QS
    &SCF
      ADDED_MOS 5
      EPS_SCF 1e-6
      MAX_SCF 40
      SCF_GUESS restart
      &MIXING
        ALPHA 0.2
      &END MIXING
      &MOM on
        DEOCC_ALPHA 3
        OCC_ALPHA 5
      &END MOM
    &END SCF
    &XC
      &XC_FUNCTIONAL pbe0
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 6.0 6.0 6.0
    &END CELL
    &COORD
      O   0.000000    0.000000   -0.075833
      H   0.000000   -0.765932    0.514713
      H   0.000000    0.765932    0.514713
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PADE
      BASIS_SET AUX_FIT SZV-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      BASIS_SET AUX_FIT SZV-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

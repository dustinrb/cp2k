&GLOBAL
  PRINT_LEVEL LOW
  PROJECT O2-UKS-NMR-4
  RUN_TYPE LINEAR_RESPONSE
&END GLOBAL

&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    MULTIPLICITY 3
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    UNRESTRICTED_KOHN_SHAM T
    &MGRID
      CUTOFF 100
      &RS_GRID
        DISTRIBUTION_TYPE DISTRIBUTED
      &END RS_GRID
    &END MGRID
    &SCF
      IGNORE_CONVERGENCE_FAILURE
      MAX_SCF 10
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &PROPERTIES
    &LINRES
      MAX_ITER 10
      PRECONDITIONER FULL_ALL
      &CURRENT
        GAUGE ATOM
        GAUGE_ATOM_RADIUS 0.5
        ORBITAL_CENTER COMMON
        USE_OLD_GAUGE_ATOM F
      &END CURRENT
      &LOCALIZE
      &END LOCALIZE
      &NMR
      &END NMR
    &END LINRES
  &END PROPERTIES
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
    &END CELL
    &COORD
      O           0.000000  0.000000  0.608000
      O           0.000000  0.000000 -0.608000
    &END COORD
    &KIND O
      BASIS_SET DZVP-GTH
      POTENTIAL GTH-BLYP-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PRINT_LEVEL MEDIUM
  PROJECT C2H4
  RUN_TYPE MD
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 2
    TEMPERATURE 300.0
    TIMESTEP 0.1
  &END MD
&END MOTION

#CPQA DEPENDS C2H4-init.inp
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    CHARGE +1
    POTENTIAL_FILE_NAME POTENTIAL
    RESTART_FILE_NAME C2H4-INIT-RESTART.wfn
    ROKS
    &MGRID
      CUTOFF 100
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-8
      &MULLIKEN_RESTRAINT
        ATOMS 1
        STRENGTH 0.3
        TARGET 1.0
      &END MULLIKEN_RESTRAINT
    &END QS
    &SCF
      IGNORE_CONVERGENCE_FAILURE
      MAX_SCF 4
      SCF_GUESS RESTART
      &OT ON
        ROTATION
      &END OT
    &END SCF
    &SIC
      SIC_METHOD MAURI_SPZ
      SIC_SCALING_A 0.2
      SIC_SCALING_B 0.0
    &END SIC
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 6.0 6.0 8.0
    &END CELL
    &COORD
      C         -0.2558206925       -0.0439868417        0.6850619693
      C         -0.2214915625       -0.0386665897       -0.6514432536
      H         -0.4100752923        0.8714166677        1.2581094627
      H         -0.1401850725       -0.9650616559        1.2579495323
      H         -0.0795226532       -0.9560364450       -1.2252531634
      H         -0.3528336094        0.8817708920       -1.2228724740
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND C
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q4
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

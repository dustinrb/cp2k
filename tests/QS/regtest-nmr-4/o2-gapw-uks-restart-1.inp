#CPQA DEPENDS o2-gapw-uks-1.inp
&GLOBAL
  PRINT_LEVEL LOW
  PROJECT o2-gapw-uks-restart-1
  RUN_TYPE LINEAR_RESPONSE
&END GLOBAL

&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    MULTIPLICITY 3
    POTENTIAL_FILE_NAME POTENTIAL
    UNRESTRICTED_KOHN_SHAM T
    &MGRID
      CUTOFF 150
    &END MGRID
    &QS
      METHOD GAPW
    &END QS
    &SCF
      IGNORE_CONVERGENCE_FAILURE
      MAX_SCF 5
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &PROPERTIES
    &LINRES
      MAX_ITER 10
      PRECONDITIONER FULL_ALL
      RESTART T
      RESTART_FILE_NAME o2-gapw-uks-1-RESTART.lr
      &CURRENT
        GAUGE R_AND_STEP_FUNCTION
      &END CURRENT
      &LOCALIZE
        MAX_ITER 10
      &END LOCALIZE
      &NMR
      &END NMR
    &END LINRES
  &END PROPERTIES
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
      O   0.000000    0.000000    0.615571
      O   0.000000    0.000000   -0.615571
    &END COORD
    &KIND O
      BASIS_SET 6-31G**
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PRINT_LEVEL LOW
  PROJECT c2h2_list-1
  RUN_TYPE LINEAR_RESPONSE
&END GLOBAL

&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 100
    &END MGRID
    &QS
      METHOD GAPW
    &END QS
    &SCF
      IGNORE_CONVERGENCE_FAILURE
      MAX_SCF 5
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &PROPERTIES
    &LINRES
      MAX_ITER 5
      PRECONDITIONER FULL_ALL
      &CURRENT
        GAUGE R_AND_STEP_FUNCTION
      &END CURRENT
      &LOCALIZE
        MAX_ITER 10
      &END LOCALIZE
      &NMR
        &PRINT
          &SHIELDING_TENSOR
            ATOMS_LIST 1 4
          &END SHIELDING_TENSOR
        &END PRINT
      &END NMR
    &END LINRES
  &END PROPERTIES
  &SUBSYS
    &CELL
      ABC 6.0 6.0 6.0
    &END CELL
    &COORD
      C   0.000000    0.000000   -0.602850
      C   0.000000    0.000000    0.602865
      H   0.000000    0.000000    1.669814
      H   0.000000    0.000000   -1.669906
    &END COORD
    &KIND H
      BASIS_SET 6-31G*
      POTENTIAL ALL
    &END KIND
    &KIND C
      BASIS_SET 6-31G*
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

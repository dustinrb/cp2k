&GLOBAL
  PRINT_LEVEL LOW
  PROJECT C2H2-PBE-ri_region
  RUN_TYPE ENERGY
&END GLOBAL

&FORCE_EVAL
  &DFT
    AUTO_BASIS RI_XAS MEDIUM
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &POISSON
      PERIODIC NONE
      PSOLVER MT
    &END POISSON
    &QS
      METHOD GAPW
    &END QS
    &XAS_TDP
      DIPOLE_FORM LENGTH
      GRID C 200 200
      N_EXCITED 12
      TAMM_DANCOFF
      &DONOR_STATES
        DEFINE_EXCITED BY_KIND
        KIND_LIST C
        LOCALIZE
        N_SEARCH 2
        STATE_TYPES 1s
      &END DONOR_STATES
      &KERNEL
        RI_REGION 3.0
        &XC_FUNCTIONAL
          &GGA_C_PBE
          &END GGA_C_PBE
          &GGA_X_PBE
          &END GGA_X_PBE
        &END XC_FUNCTIONAL
      &END KERNEL
    &END XAS_TDP
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
      PERIODIC NONE
    &END CELL
    &COORD
      H  0.000000    0.000000    1.666650
      C  0.000000    0.000000    0.603250
      C  0.000000    0.000000   -0.603250
      H  0.000000    0.000000   -1.666650
    &END COORD
    &KIND C
      BASIS_SET 6-31G*
      POTENTIAL ALL
    &END KIND
    &KIND H
      BASIS_SET 3-21G*
      POTENTIAL ALL
    &END KIND
    &TOPOLOGY
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PRINT_LEVEL LOW
  PROJECT TEST
  RUN_TYPE RT_PROPAGATION
&END GLOBAL

&MOTION
  &MD
    ENSEMBLE NVE
    STEPS 2
    TEMPERATURE 300.0
    TIMESTEP [au_t] 0.25
  &END MD
&END MOTION

&FORCE_EVAL
  METHOD QUICKSTEP
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    CHARGE +1
    LSD
    POTENTIAL_FILE_NAME POTENTIAL
    &LS_SCF
      EPS_FILTER 1.0E-10
      EPS_LANCZOS 1.0E-8
      MAX_ITER_LANCZOS 30
      SIGN_SQRT_ORDER 3
    &END LS_SCF
    &MGRID
      CUTOFF 100
    &END MGRID
    &POISSON
      PERIODIC NONE
      POISSON MT
    &END POISSON
    &QS
      EPS_DEFAULT 1.0E-10
      LS_SCF
    &END QS
    &REAL_TIME_PROPAGATION
      DENSITY_PROPAGATION
      EPS_ITER 1.0E-9
      INITIAL_WFN SCF_WFN
      MAT_EXP TAYLOR
      MAX_ITER 5
      MCWEENY_MAX_ITER 1
      &PRINT
        &CURRENT
          STRIDE 3 3 3
          &EACH
            MD 1
          &END EACH
        &END CURRENT
      &END PRINT
    &END REAL_TIME_PROPAGATION
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 6.0 6.0 6.0
      PERIODIC NONE
    &END CELL
    &COORD
      H  6.000000   4.5000000   6.000000
      H  6.000000   1.5000000    6.000000
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PBE
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND HX
      BASIS_SET DZVP-GTH-PBE
      ELEMENT H
      GHOST T
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PBE
      POTENTIAL GTH-PBE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

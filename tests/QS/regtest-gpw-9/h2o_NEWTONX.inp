&GLOBAL
  PREFERRED_DIAG_LIBRARY SL
  PRINT_LEVEL medium
  PROJECT test
  RUN_TYPE VIBRATIONAL_ANALYSIS
&END GLOBAL

&VIBRATIONAL_ANALYSIS
  DX 0.001
  &PRINT
    &NAMD_PRINT
    &END NAMD_PRINT
  &END PRINT
&END VIBRATIONAL_ANALYSIS

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    &QS
      METHOD xTB
    &END QS
    &SCF
      EPS_SCF 1.0E-8
      MAX_SCF 10
      SCF_GUESS MOPAC
      &OT
        MINIMIZER DIIS
        PRECONDITIONER FULL_ALL
      &END OT
      &OUTER_SCF
        EPS_SCF 1.0E-8
        MAX_SCF 10
      &END OUTER_SCF
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC [angstrom] 6.0 6.0 6.0
      PERIODIC NONE
    &END CELL
    &COORD
      UNIT angstrom
      O   0.000000    0.000000    0.000000
      H   0.000000   -0.757136    0.580545
      H   0.000000    0.757136    0.580545
    &END COORD
    &TOPOLOGY
      &CENTER_COORDINATES T
      &END CENTER_COORDINATES
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL

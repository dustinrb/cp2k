&GLOBAL
  PRINT_LEVEL LOW
  PROJECT HF_dipole
  RUN_TYPE ENERGY
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ALL_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 200
    &END MGRID
    &PRINT
      &MOMENTS
        MAX_MOMENT 1
        PERIODIC F
      &END MOMENTS
      &VORONOI
        MOLECULAR_PROPERTIES
      &END VORONOI
    &END PRINT
    &QS
      METHOD GAPW
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      MAX_SCF 20
      SCF_GUESS ATOMIC
      &OT
        MINIMIZER DIIS
        PRECONDITIONER FULL_ALL
      &END OT
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 8.0 8.0 8.0
    &END CELL
    &COORD
      F 0.0 0.0 0.0    HF
      H 0.0 0.0 0.917  HF
    &END COORD
    &KIND F
      BASIS_SET DZVP-ALL-PADE
      POTENTIAL ALL
    &END KIND
    &KIND H
      BASIS_SET DZVP-ALL-PADE
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

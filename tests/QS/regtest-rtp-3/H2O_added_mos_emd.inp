&GLOBAL
  PRINT_LEVEL MEDIUM
  PROJECT H2O_excit_emd
  RUN_TYPE EHRENFEST_DYN
&END GLOBAL

&MOTION
  &MD
    STEPS 2
    TIMESTEP [au_t] 0.1
  &END MD
&END MOTION

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    LSD
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 150
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-12
    &END QS
    &REAL_TIME_PROPAGATION
      EPS_ITER 1.0E-9
      EXP_ACCURACY 1.0E-9
      INITIAL_WFN SCF_WFN
      MAT_EXP PADE
      MAX_ITER 8
      PROPAGATOR ETRS
    &END REAL_TIME_PROPAGATION
    &SCF
      ADDED_MOS 1
      EPS_SCF 3.0E-7
      MAX_SCF 50
      SCF_GUESS ATOMIC
      &DIAGONALIZATION
        ALGORITHM STANDARD
      &END DIAGONALIZATION
    &END SCF
    &XC
      &XC_FUNCTIONAL BLYP
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
    &END CELL
    &COORD
      O   0.000000    0.000000   -0.065587 H2O
      H   0.000000   -0.757136    0.520545 H2O
      H   0.000000    0.757136    0.520545 H2O
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-BLYP
      POTENTIAL GTH-BLYP-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-BLYP
      POTENTIAL GTH-BLYP-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PRINT_LEVEL LOW
  PROJECT FeH_8cat
  RUN_TYPE ENERGY_FORCE
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    CHARGE 8
    LSD
    &PRINT
      &AO_MATRICES
        CORE_HAMILTONIAN
        DENSITY
        KOHN_SHAM_MATRIX
        OVERLAP
      &END AO_MATRICES
    &END PRINT
    &QS
      METHOD PM6
      &SE
      &END SE
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      MAX_SCF 50
      SCF_GUESS mopac
      &MIXING
        ALPHA 0.1
        METHOD DIRECT_P_MIXING
      &END MIXING
      &OUTER_SCF
        EPS_SCF 1.0E-6
        MAX_SCF 5
      &END OUTER_SCF
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC 10.0 10.0 10.0
      PERIODIC NONE
    &END CELL
    &COORD
      Fe    0.000000     0.000000     0.000000
      H     1.300000     0.000000     0.000000
    &END COORD
    &PRINT
      &KINDS
        BASIS_SET
        POTENTIAL
        SE_PARAMETERS
      &END KINDS
    &END PRINT
  &END SUBSYS
&END FORCE_EVAL

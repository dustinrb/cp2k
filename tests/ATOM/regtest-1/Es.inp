&GLOBAL
  PROGRAM_NAME ATOM
  PROJECT Es
&END GLOBAL

&ATOM
  ELECTRON_CONFIGURATION [Rn] 5f11 7s2
  ELEMENT Es
  &AE_BASIS
    BASIS_TYPE GEOMETRICAL_GTO
    GEOMETRICAL_FACTOR 3.2
    GEO_START_VALUE 0.2
    NUM_GTO 10 10 5 5
    START_INDEX 0 0 4 4
  &END AE_BASIS
  &METHOD
    METHOD_TYPE KOHN-SHAM
    RELATIVISTIC ZORA(MP)
    &XC
      &XC_FUNCTIONAL
        &VWN
        &END VWN
      &END XC_FUNCTIONAL
    &END XC
  &END METHOD
&END ATOM

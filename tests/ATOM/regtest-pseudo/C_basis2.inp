&GLOBAL
  PROGRAM_NAME ATOM
  PROJECT C
&END GLOBAL

&ATOM
  ELECTRON_CONFIGURATION 1s2 2s2 2p2
  ELEMENT C
  &AE_BASIS
    BASIS_SET 6-31G*
    BASIS_TYPE CONTRACTED_GTO
  &END AE_BASIS
  &METHOD
    METHOD_TYPE KOHN-SHAM
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END METHOD
  &PRINT
    &ANALYZE_BASIS
      COMPLETENESS T
      OVERLAP_CONDITION_NUMBER T
    &END ANALYZE_BASIS
  &END PRINT
&END ATOM

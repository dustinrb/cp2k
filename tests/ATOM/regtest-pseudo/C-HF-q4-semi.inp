&GLOBAL
  PROGRAM_NAME ATOM
&END GLOBAL

&ATOM
  CORE [He]
  COULOMB_INTEGRALS ANALYTIC
  ELECTRON_CONFIGURATION [He] 2s2 2p2
  ELEMENT C
  EXCHANGE_INTEGRALS SEMI_ANALYTIC
  MAX_ANGULAR_MOMENTUM 2
  RUN_TYPE PSEUDOPOTENTIAL_OPTIMIZATION
  &AE_BASIS
    BASIS_TYPE GEOMETRICAL_GTO
    GRID_POINTS 200
  &END AE_BASIS
  &METHOD
    METHOD_TYPE HARTREE-FOCK
  &END METHOD
  &OPTIMIZATION
    EPS_SCF 1.e-5
  &END OPTIMIZATION
  &POTENTIAL
    PSEUDO_TYPE GTH
    &GTH_POTENTIAL
      2    2    0    0
      0.315416720946       2   -8.867310835419    1.255274898432
      1
      0.301022699841       1    9.800186386978
    &END GTH_POTENTIAL
  &END POTENTIAL
  &POWELL
    ACCURACY 1.e-0
    STEP_SIZE 0.75
    WEIGHT_PSIR0 0.1
  &END POWELL
  &PP_BASIS
    BASIS_TYPE GEOMETRICAL_GTO
  &END PP_BASIS
  &PRINT
    &BASIS_SET
    &END BASIS_SET
  &END PRINT
&END ATOM

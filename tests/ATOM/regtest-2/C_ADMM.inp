&GLOBAL
  PROGRAM_NAME ATOM
&END GLOBAL

&ATOM
  ELECTRON_CONFIGURATION [He] 2s2 2p2
  ELEMENT C
  MAX_ANGULAR_MOMENTUM 1
  RUN_TYPE ENERGY
  &AE_BASIS
    BASIS_SET aug-cc-pVTZ
    BASIS_SET_FILE_NAME EMSL_BASIS_SETS
    BASIS_TYPE CONTRACTED_GTO
  &END AE_BASIS
  &METHOD
    METHOD_TYPE KOHN-SHAM
    RELATIVISTIC DKH(3)
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END METHOD
  &OPTIMIZATION
    EPS_SCF 1.e-8
  &END OPTIMIZATION
  &PRINT
    &ADMM
      &ADMM_BASIS
        BASIS_SET 3-21Gx
        BASIS_SET_FILE_NAME EMSL_BASIS_SETS
        BASIS_TYPE CONTRACTED_GTO
      &END ADMM_BASIS
    &END ADMM
  &END PRINT
&END ATOM

&GLOBAL
  PROGRAM_NAME ATOM
&END GLOBAL

&ATOM
  ELECTRON_CONFIGURATION 1s2
  ELEMENT He
  MAX_ANGULAR_MOMENTUM 0
  RUN_TYPE ENERGY
  &AE_BASIS
    BASIS_TYPE SLATER
    NUM_SLATER 1 0 0 0
    S_EXPONENTS 1.6875
    S_QUANTUM_NUMBERS 1
  &END AE_BASIS
  &METHOD
    METHOD_TYPE HARTREE-FOCK
  &END METHOD
  &OPTIMIZATION
    EPS_SCF 1.e-12
  &END OPTIMIZATION
  &POTENTIAL
    CONFINEMENT 1.0 5.0 4
    CONFINEMENT_TYPE POLYNOM
  &END POTENTIAL
&END ATOM
